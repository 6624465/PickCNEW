
@{
    ViewBag.Title = "Index";
    Layout = "~/Areas/OperatorWeb/Views/Shared/_Layout.cshtml";
}
@section seccssOW{
    <link href="@Url.Content("~/Css/bootstrap-datetimepicker.css")" rel="stylesheet" />
}
<div class="container">
    <div class="row marginTop">
        <div class="col-md-6 col-xs-6">
            <div class="form-group">
                <div class='input-group date' id='datetimepicker1'>
                    <input type='text' class="form-control input-sm" />
                    <span class="input-group-addon">
                        <span class="fa fa-clock-o"></span>
                    </span>
                </div>
            </div>
        </div>
        <div class="col-md-6 col-xs-6">
            <select class="form-control input-sm"></select>
        </div>
    </div>
    <div class="row">
        <div class="col-md-12 col-xs-12 text-center">
            <div class="well well-sm">
                <h4 class="box-title">You have earned</h4>
                <h3><i class="fa fa-rupee"></i>&nbsp;0.00</h3>
            </div>            
        </div>
    </div>
    <div class="row">
        <div class="col-md-12 col-xs-12">
            <h4 class="box-title">Account Summary</h4>
        </div>
    </div>
    <div class="row">
        <div class="col-md-12 col-xs-12">
            <h4 class="box-title">Bill Amount<span class="pull-right"><i class="fa fa-rupee"></i>&nbsp;0.00</span></h4>
        </div>
    </div>
    <div class="row">
        <div class="col-md-12 col-xs-12">
            <h4 class="box-title">Ride Earnings<span class="pull-right"><i class="fa fa-rupee"></i>&nbsp;0.00</span></h4>
        </div>
    </div>
    <div class="row">
        <div class="col-md-12 col-xs-12">
            <h4 class="box-title">Incentives<span class="pull-right"><i class="fa fa-rupee"></i>&nbsp;0.00</span></h4>
        </div>
    </div>
    <div class="row">
        <div class="col-md-12 col-xs-12">
            <h4 class="box-title">Tolls and Parking<span class="pull-right"><i class="fa fa-rupee"></i>&nbsp;0.00</span></h4>
        </div>
    </div>
    <div class="row">
        <div class="col-md-12 col-xs-12">
            <h4 class="box-title">Other Earnings<span class="pull-right"><i class="fa fa-rupee"></i>&nbsp;0.00</span></h4>
        </div>
    </div>
    <div class="row">
        <div class="col-md-12 col-xs-12">
            <h4 class="box-title">Total Earnings<span class="pull-right"><i class="fa fa-rupee"></i>&nbsp;0.00</span></h4>
        </div>
    </div>
    <div class="row">
        <div class="col-md-12 col-xs-12">
            <h4 class="box-title">Deductions<span class="pull-right"><i class="fa fa-rupee"></i>&nbsp;0.00</span></h4>
        </div>
    </div>
</div>
@section secscriptsOW{
    <script src="@Url.Content("~/Scripts/moment.js")"></script>
    <script src="@Url.Content("~/Scripts/bootstrap-datetimepicker.js")"></script>
    <script type="text/javascript">
        $(function () {
            $('#datetimepicker1').datetimepicker();
        });
    </script>
}




